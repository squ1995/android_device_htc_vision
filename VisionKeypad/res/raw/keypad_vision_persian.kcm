}
key W              {
	label:'W'       
	base:'\u0635'       
	shift, capslock:'W'       
	alt: '\u06f2'       
	shift+alt:'2'       
}
key X              {
	label:'X'       
	base:'\u0637'       
	shift, capslock:'X'       
	alt: '\u20AC'  
	shift+alt:'\u20AC'  
}
key Y              {
	label:'Y'       
	base:'\u063a'       
	shift, capslock:'Y'       
	alt: '\u06f6'       
	shift+alt:'6'       
}
key Z              {
	label:'Z'       
	base:'\u0638'       
	shift, capslock:'Z'       
	alt: '\u00A3'  
	shift+alt:'\u00A3'  
} 
key BUTTON_3             {
        label:':'       
        base:':'       
        shift, capslock:':'       
        alt: ':'  
        shift+alt:':'  
}
key BUTTON_5             {
        label:'\u0416'       
        base:'\u0436'       
        shift, capslock:'\u0416'       
        alt: '|'  
        shift+alt:'|'  
}
key BUTTON_R2 {
  label:'?'
  base:'\u0648'
  shift, capslock:'?'
  alt: '"'
  shift+alt: '<'
}
key PERIOD         {
	label:'.'       
	base:'.'       
	shift, capslock:'.'       
	alt: ','       
	shift+alt:','       
}
key TAB            {
	label:'\u0009'  
	base:'\u0009'  
	shift, capslock:'\u0009'  
	alt: '\u0009'  
	shift+alt:'\u0009'  
}
key SPACE          {
	label:'\u0020'  
	base:'\u0020'  
	shift, capslock:'\u0020'  
	alt: '\u0020'  
	shift+alt:'\u0020'  
}
key ENTER          {
	label:'\u000A'  
	base:'\u000A'  
	shift, capslock:'\u000A'  
	alt: '\u000A'  
	shift+alt:'\u000A'  
}

key SEMICOLON      {
	label:';'       
	base:';'       
	shift, capslock:':'       
	alt: '?'       
	shift+alt:'>'       
}
key AT             {
	label:'@'       
	base:'\u067e'       
	shift, capslock:'@'       
	alt: '~'       
	shift+alt:'`'       
}
key SEARCH         {
  shift: '\uEF00'
	alt: '\uEF01'  
	shift+alt:'\uEF01'  
}
