}
key Q              {
	label:'Q'       
	base:'\u0636'       
	shift, capslock:'Q'       
	alt: '\u06f1'       
	shift+alt:'1'       
}
key R              {
	label:'R'       
	base:'\u0642'       
	shift, capslock:'R'       
	alt: '\u06f4'       
	shift+alt:'4'       
}
key S              {
	label:'S'       
	base:'\u0633'       
	shift, capslock:'S'       
	alt: '#'       
	shift+alt:'#'       
}
key T              {
	label:'T'       
	base:'\u0641'       
	shift, capslock:'T'       
	alt: '\u06f5'       
