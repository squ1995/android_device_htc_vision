type OVERLAY
key A              {
	label:'A'       
	base:'\u0634'       
	shift, capslock:'A'       
	alt: '!'       
	shift+alt:'!'       
}
key B              {
	label:'B'       
	base:'\u0630'       
	shift, capslock:'B'       
	alt: '+'       
	shift+alt:'+'       
}
key C              {
	label:'C'       
	base:'\u0632'       
	shift, capslock:'\u0698'       
	alt: '_'       
	shift+alt:'_'       
}
key D              {
	label:'D'       
	base:'\u06cc'       
	shift, capslock:'D'       
	alt: '$'       
	shift+alt:'$'       
}
key E              {
	label:'E'       
	base:'\u062b'       
	shift, capslock:'E'       
	alt: '\u06f3'       
	shift+alt:'3'       
}
key F              {
	label:'F'       
	base:'\u0628'       
	shift, capslock:'F'       
	alt: '%'       
	shift+alt:'%'       
}
key G              {
	label:'G'       
	base:'\u0644'       
	shift, capslock:'G'       
	alt: '&'       
	shift+alt:'['       
}
key H              {
	label:'H'       
	base:'\u0627'       
	shift, capslock:'H'       
	alt: '*'       
	shift+alt:']'       
}
key I              {
	label:'I'       
	base:'\u0647'       
	shift, capslock:'I'       
	alt: '\u06f8'       
	shift+alt:'8'       
}
key J              {
	label:'J'       
	base:'\u062a'       
	shift, capslock:'J'       
	alt: '('       
	shift+alt:'{'       
}
key K              {
	label:'K'       
	base:'\u0646'       
	shift, capslock:'K'       
	alt: ')'       
	shift+alt:'}'       
}
key L              {
	label:'L'       
	base:'\u0645'       
	shift, capslock:'L'       
	alt: '/'       
	shift+alt:'\\'       
}
key M              {
	label:'M'       
	base:'\u067e'       
	shift, capslock:'M'       
	alt: '\''
	shift+alt:'\''       
}
key N              {
	label:'N'       
	base:'\u062f'       
	shift, capslock:'N'       
	alt: '='      
	shift+alt:'='      
}
key O              {
	label:'O'       
	base:'\u062e'       
	shift, capslock:'O'       
	alt: '\u06f9'       
